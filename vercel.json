{
  "version": 2,
  "name": "ai-doc-analyser-frontend",
  "buildCommand": "cd ai-doc-analyser-frontend && npm install && npm run build",
  "outputDirectory": "ai-doc-analyser-frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
