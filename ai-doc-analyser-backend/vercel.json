{
  "version": 2,
  "name": "ai-doc-analyser-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
