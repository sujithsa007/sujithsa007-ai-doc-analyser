{
  "version": 2,
  "name": "ai-doc-analyser-backend",
  "buildCommand": "echo 'No build step required'",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" != \"main\" ]; then exit 0; else exit 1; fi",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
